<!-- FIRST NAME -->
<%= render "layouts/form/texfield", f: f, input: {id: :first_name} %>
  
<!-- LAST FIRST NAME -->
<%= render "layouts/form/texfield", f: f, input: {id: :first_last_name} %>

<!-- AGE -->
<%= render "layouts/form/texfield", f: f, input: {id: :age, pattern: {value: "-?[0-9]*(\.[0-9]+)?"}} %>
  
<!-- SEX -->
<%= render "layouts/form/select", f: f, input: {id: :sex}, options: Client.sexValue %>